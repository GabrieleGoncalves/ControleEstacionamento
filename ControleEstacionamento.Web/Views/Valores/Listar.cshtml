@model IEnumerable<ControleEstacionamento.Web.ViewModels.Valores.ValoresViewModelList>


@{
    ViewBag.Title = "Listar";
}

@if (Model.Count() > 0)
{
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="panel-title col-md-6">
               
                <br />
                <br />
                    <strong>Valor da Hora: </strong> @item.ValorHora
                    <br />
                    <br />
                    <strong>Início de Vigência: </strong> @item.InicioVigencia
                    <br />
                    <br />
                    <strong>Fim de Vigência: </strong> @item.FimVigencia
                    <br />
                    <br />
                    <strong>Valor da Hora Adicional: </strong> @item.ValorHoraAdicional

            </div> 
        }
        
    </div>

}




